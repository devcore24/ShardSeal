{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "HTTP Requests Rate by Code",
      "gridPos": { "w": 12, "h": 8, "x": 0, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (code) (rate(shardseal_http_requests_total{job=~\"$job\"}[$__rate_interval]))",
          "legendFormat": "{{code}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "req/s" }, "overrides": [] },
      "options": { "legend": { "displayMode": "table", "placement": "right" } }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "HTTP 5xx Error Ratio (5m)",
      "gridPos": { "w": 12, "h": 8, "x": 12, "y": 0 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(shardseal_http_requests_total{job=~\"$job\",code=~\"5..\"}[$__rate_interval])) / sum(rate(shardseal_http_requests_total{job=~\"$job\"}[$__rate_interval]))"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green" }, { "color": "orange", "value": 0.02 }, { "color": "red", "value": 0.05 }] },
          "decimals": 2
        },
        "overrides": []
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Sealed Integrity Failures (rate)",
      "gridPos": { "w": 24, "h": 8, "x": 0, "y": 8 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        {
          "refId": "A",
          "expr": "rate(shardseal_storage_integrity_failures_total{job=~\"$job\"}[$__rate_interval])",
          "legendFormat": "integrity_failures"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "1/s" }, "overrides": [] },
      "options": { "legend": { "displayMode": "list", "placement": "bottom" } }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "HTTP p99 Latency by Method",
      "gridPos": { "w": 24, "h": 8, "x": 0, "y": 16 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.99, sum by (le, method) (rate(shardseal_http_request_duration_seconds_bucket{job=~\"$job\"}[$__rate_interval])))",
          "legendFormat": "{{method}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "s" }, "overrides": [] },
      "options": { "legend": { "displayMode": "table", "placement": "right" } }
    },
    {
      "id": 5,
      "type": "stat",
      "title": "Sealed Ops Integrity Fail Ratio (5m)",
      "gridPos": { "w": 12, "h": 8, "x": 0, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(shardseal_storage_sealed_ops_total{job=~\"$job\",integrity_fail=\"true\"}[$__rate_interval])) / sum(rate(shardseal_storage_sealed_ops_total{job=~\"$job\"}[$__rate_interval]))"
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "orientation": "auto",
        "textMode": "value"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green" }, { "color": "orange", "value": 0.01 }, { "color": "red", "value": 0.05 }] },
          "decimals": 2
        },
        "overrides": []
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Storage Throughput (bytes/s) by op",
      "gridPos": { "w": 12, "h": 8, "x": 12, "y": 24 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        {
          "refId": "A",
          "expr": "sum by (op) (rate(shardseal_storage_bytes_total{job=~\"$job\"}[$__rate_interval]))",
          "legendFormat": "{{op}}"
        }
      ],
      "fieldConfig": { "defaults": { "unit": "Bps" }, "overrides": [] },
      "options": { "legend": { "displayMode": "table", "placement": "right" } }
    },

    {
      "id": 7,
      "type": "stat",
      "title": "Scrubber Errors Rate (5m)",
      "gridPos": { "w": 6, "h": 6, "x": 0, "y": 32 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        { "refId": "A", "expr": "rate(shardseal_scrubber_errors_total{job=~\"$job\"}[$__rate_interval])" }
      ],
      "fieldConfig": { "defaults": { "unit": "1/s", "thresholds": { "mode": "absolute", "steps": [ { "color": "green" }, { "color": "red", "value": 0 } ] } }, "overrides": [] },
      "options": { "colorMode": "value", "graphMode": "none", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false } }
    },
    {
      "id": 8,
      "type": "stat",
      "title": "Scrubber Scanned Rate (5m)",
      "gridPos": { "w": 6, "h": 6, "x": 6, "y": 32 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        { "refId": "A", "expr": "rate(shardseal_scrubber_scanned_total{job=~\"$job\"}[$__rate_interval])" }
      ],
      "fieldConfig": { "defaults": { "unit": "1/s" }, "overrides": [] },
      "options": { "colorMode": "value", "graphMode": "none", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false } }
    },
    {
      "id": 9,
      "type": "stat",
      "title": "Scrubber Last Run (minutes ago)",
      "gridPos": { "w": 6, "h": 6, "x": 12, "y": 32 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        { "refId": "A", "expr": "(time() - shardseal_scrubber_last_run_timestamp_seconds{job=~\"$job\"}) / 60" }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "m",
          "thresholds": { "mode": "absolute", "steps": [ { "color": "green" }, { "color": "orange", "value": 60 }, { "color": "red", "value": 120 } ] }
        },
        "overrides": []
      },
      "options": { "colorMode": "value", "graphMode": "none", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false } }
    },
    {
      "id": 10,
      "type": "stat",
      "title": "Scrubber Uptime (hours)",
      "gridPos": { "w": 6, "h": 6, "x": 18, "y": 32 },
      "datasource": { "type": "prometheus", "uid": "$DS_PROMETHEUS" },
      "targets": [
        { "refId": "A", "expr": "shardseal_scrubber_uptime_seconds{job=~\"$job\"} / 3600" }
      ],
      "fieldConfig": { "defaults": { "unit": "h" }, "overrides": [] },
      "options": { "colorMode": "value", "graphMode": "none", "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false } }
    }
  ],
  "refresh": "30s",
